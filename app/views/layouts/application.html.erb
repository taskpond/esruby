<!DOCTYPE html>
<html>
<head>
  <title>ElasticsearchRuby</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 	 'flatly' %>
  <%= yield :stylesheets %>

  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'flatly' %>
  <%= yield :javascripts %>

  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<%= render 'shared/navigator' %>
		<%= yield %>				
	</div>
</body>
</html>
